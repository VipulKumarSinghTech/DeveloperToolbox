<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="10" stylesheets="@uuid-generator.css" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.vipulkumarsinghtech.developertoolbox.UuidGeneratorController">
    <Label text="Random UUID generator:"/>
    <Button onAction="#generateUuid" styleClass="buttonClass" text="Generate"/>
    <Label text="UUID:"/>
    <TextArea fx:id="uuid" styleClass="uuid"  promptText="UUID"/>
</VBox>
